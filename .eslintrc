{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "google",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "import"
    ],
    "rules": {
      "guard-for-in": 0,
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        2,
        "windows"
      ],
      "max-len": 0,
      "new-cap": [
        2,
        {
          "capIsNew": false
        }
      ],
      "no-invalid-this": 0,
      "no-var": 0,
      "one-var": 0,
      "prefer-rest-params": 0,
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    }
}
